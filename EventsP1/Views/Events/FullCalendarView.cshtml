@model EventsP1.Models.Event

@{
    ViewBag.Title = "FullCalendarView";
}

<h2>FullCalendarView</h2>

@section scripts{


<script>
    $(document).ready(function () {



        $('#calendar').fullCalendar({
            header: {
                left: 'prev,next today',
                center: 'title',
                right: 'month,agendaWeek,agendaDay'
            },
            defaultView: 'agendaDay',
            editable: true,
            allDaySlot: false,
            selectable: true,
            slotMinutes: 15,

        });
    })
</script>
   
     }

<div id='calendar' style="width:65%"></div>

